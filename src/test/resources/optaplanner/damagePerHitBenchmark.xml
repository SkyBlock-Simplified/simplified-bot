                <easyScoreCalculatorClass>dev.sbs.simplifiedbot.optimizer.modules.damage_per_hit.DamagePerHitCalculator</easyScoreCalculatorClass>
            </scoreDirectorFactory>
            <termination>
                <secondsSpentLimit>5</secondsSpentLimit>
            </termination>
        </solver>
        <problemBenchmarks>
            <problemStatisticType>BEST_SCORE</problemStatisticType>
        </problemBenchmarks>
    </inheritedSolverBenchmark>
